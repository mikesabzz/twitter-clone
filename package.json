{
  "name": "twitter-clone",
  "version": "1.0.0",
  "description": "Remake of Twitter with Express, Auth, PassportJS, SQL, and React.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "heroku-postbuild": "cd client/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run client\"",
    "db:init": "dropdb --if-exists twitter_clone_db && createdb twitter_clone_db",
    "db:reset": "node ./scripts/resetDb.js",
    "db:seed": "node ./scripts/seedDb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikesabzz/twitter-clone.git"
  },
  "author": "Michael Sabzevari",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikesabzz/twitter-clone/issues"
  },
  "homepage": "https://github.com/mikesabzz/twitter-clone#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "fs": "0.0.1-security",
    "init": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node": "^13.6.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.0.2",
    "pq": "0.0.3",
    "react-move": "^6.1.0",
    "react-portal": "^4.2.1",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.6",
    "server.js": "^1.0.0",
    "swagger-ui-dist": "^3.25.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
